<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.td.mylevelup">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity android:name=".dashboard.DashboardActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".accounts.AccountsActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".creditCard.CreditCardActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".investing.searchSymbolsPage.InvestingActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".investing.simulationResultsPage.InvestingSimulationActivity"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>